@model NewsForums.Models.ApplicationUser.ProfileListModel 
<div class="container body-content">

    <table>
        <thead>
            <tr>
                <th>Image </th>
                <th>UserName </th>
                <th>Email </th>
                <th>Memember Since </th>
                <th>Rating </th>
            </tr>
        </thead>
        <tbody>
            @foreach(var profile in Model.Profiles)
            {
                <tr>
                    <td>@profile.ImageUpload</td>
                    <td>@profile.UserName</td>
                    <td>@profile.Email</td>
                    <td>@profile.MemberSince</td>
                    <td>@profile.UserRating</td>
                </tr>
            }
        </tbody>foreach
    </table>


</div>